<Window x:Class="PracticaStackPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaStackPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <Button VerticalAlignment="Bottom">Boton 1</Button>
        <Button VerticalAlignment="Stretch">Boton 2</Button>
        <Button VerticalAlignment="Center">Boton 3</Button>
        <Button VerticalAlignment="Top">Boton 4</Button>
        <Button HorizontalAlignment="Stretch">Boton 5</Button>
        <Button HorizontalAlignment="Left">Boton 6</Button>
        <Button HorizontalAlignment="Right" Width="270">Boton 7</Button>
        <Button HorizontalAlignment="Center">Boton 8</Button>
        <Button Width="290">Boton 9</Button>
        <Button>Boton 10</Button>
        <Button>Boton 11</Button>
        <Button>Boton 12</Button>
        <Button>Boton 13</Button>
    </StackPanel>
</Window>
